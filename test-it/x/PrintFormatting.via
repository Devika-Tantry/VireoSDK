define (HelloWorld v(.VirtualInstrument (
    c(
        e(v(.String "abc") vStr)
        e(v(.Int32 427) vInt)
        e(v(.Double 42.5) vReal)
    )
    1
    clump (1
        Printf("String with default justify   -----------")
        Printf("String no field width          -%s-" vStr)
        Printf("String to samll field width    -%1s-" vStr)
        Printf("String matching field width    -%3s-" vStr)
        Printf("String larger   field width    -%4s-" vStr)
        Printf("String xlarge   field width    -%50s-" vStr)

        Printf("String with left justify set  -----------")
        Printf("String no field width          -%-s-" vStr)
        Printf("String to samll field width    -%-1s-" vStr)
        Printf("String matching field width    -%-3s-" vStr)
        Printf("String larger   field width    -%-4s-" vStr)
        Printf("String xlarge   field width    -%-50s-" vStr)

        Printf("Int with default justify      -----------")
        Printf("Int no field width          -%d-" vInt)
        Printf("Int to samll field width    -%1d-" vInt)
        Printf("Int matching field width    -%3d-" vInt)
        Printf("Int larger   field width    -%4d-" vInt)
        Printf("Int xlarge   field width    -%50d-" vInt)

        Printf("Int with left justify set     -----------")
        Printf("Int no field width          -%-d-" vInt)
        Printf("Int to samll field width    -%-1d-" vInt)
        Printf("Int matching field width    -%-3d-" vInt)
        Printf("Int larger   field width    -%-4d-" vInt)
        Printf("Int xlarge   field width    -%-50d-" vInt)

        Printf("Real with default justify      -----------")
        Printf("Real no field width          -%.0f-" vReal)
        Printf("Real to samll field width    -%1.0f-" vReal)
        Printf("Real matching field width    -%3.0f-" vReal)
        Printf("Real larger   field width    -%4.0f-" vReal)
        Printf("Real xlarge   field width    -%50.0f-" vReal)

        Printf("Real with left justify set     -----------")
        Printf("Real no field width          -%-.0f-" vReal)
        Printf("Real to samll field width    -%-1.0f-" vReal)
        Printf("Real matching field width    -%-3.0f-" vReal)
        Printf("Real larger   field width    -%-4.0f-" vReal)
        Printf("Real xlarge   field width    -%-50.0f-" vReal)

        Printf("Real with default justify      -----------")
        Printf("Real no field width          -%.1f-" vReal)
        Printf("Real to samll field width    -%1.1f-" vReal)
        Printf("Real matching field width    -%4.1f-" vReal)
        Printf("Real larger   field width    -%5.1f-" vReal)
        Printf("Real xlarge   field width    -%50.1f-" vReal)

        Printf("Real with left justify set     -----------")
        Printf("Real no field width          -%-.1f-" vReal)
        Printf("Real to samll field width    -%-1.1f-" vReal)
        Printf("Real matching field width    -%-4.1f-" vReal)
        Printf("Real larger   field width    -%-5.1f-" vReal)
        Printf("Real xlarge   field width    -%-50.1f-" vReal)

    )
) ) )
enqueue (HelloWorld)