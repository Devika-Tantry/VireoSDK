// Put Top VI in the middle
define (TopVI, dv(.VirtualInstrument, (
 c(
    e(dv(.String,'Before call'), sBefore)
    e(dv(.String,'After call'), sAfter)
  ),
  1,
  clump(1,
    Print(sBefore)
    SubVI1()
    SubVI2()
   // CallVI(SubVI3)
   // CallVI(SubVI4)
    Print(sAfter)
   )
 )))
 
