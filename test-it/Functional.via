// Enqueue can take a VI as a typed literal.
enqueue(VirtualInstrument< (
    clump(
        // Print a simple string literal.
        Println("Hello, anonymous function.")

        // Print a simple numeric literal. Its an Int32.
        Println(5)

        // Print a typed literal.
        Println(Int8<5>)

        // VI typed literals can be parameters as well.
        Start(VirtualInstrument< (clump(Println("Hi Dustyn!"))) > )
    )
) > )
