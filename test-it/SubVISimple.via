define (TopVI dv(.VirtualInstrument (
 c(
   e(dv(.Int32 42) value)
   e(dv(.Double 42.1) dValue)
    e(dv(.String 'Before call') sBefore)
    e(dv(.String 'After call')  sAfter)
  )
  1 
    clump(1 
    SubVI0()
    Print(sBefore)
    SubVI1(value)
    Print(value)
    SubVI2(value value)
    Print(value)
    SubVI3(value) // in-out parameter
    Print(value)
    Print(sAfter)
    
    SubVI1d(dValue)
    SubVI2d(dValue dValue)
    SubVI3d(dValue)
   )
 )))

enqueue (TopVI)
