    e(.Double dvalue2)
    e(.String outString)
    e(v(.String,'123 456 abc'), cString)
    e(v(.String,'1.23 4.56 6.78'), dString)
    e(v(.Int16,4) width)
    e(v(.ComplexDouble (1.2 2.3)) cDouble)

    e(.Single local1)
    e(.Int32 local2)
 ),
  1,
  clump(1,
    // Int32 to String
    Print(cn2)
    ToString(cn2,width,outString)
    Print(outString)

    Print(c3)
    ToString(c3,width,outString)
    Print(outString)

    //---------------------------------------------------------

    // Double to String
    Print(d1_5)
    ToString(d1_5,width,outString)
    Print(outString)

    Print(d1_76)
    ToString(d1_76,width,outString)
    Print(outString)

    //---------------------------------------------------------

    // String to Int32
    Print(cString)
    Copy(c0 offset)

    DecimalStringToNumber(cString offset defaultVal endoffset value)
    Print(offset)
    Print(endoffset)
    Print(value)

    Copy(endoffset, offset)
    DecimalStringToNumber(cString offset defaultVal endoffset value)
    Print(offset)
    Print(endoffset)
    Print(value)

    Copy(endoffset, offset)
    DecimalStringToNumber(cString offset defaultVal endoffset value)
    Print(offset)
    Print(endoffset)
    Print(value)

    Copy(endoffset, offset)
    DecimalStringToNumber(cString,offset,*,*,value)
    Print(offset)
    Print(endoffset)
    Print(value)

    //---------------------------------------------------------

    // String to Double
    Print(dString)
    Copy(c0 offset)

    ExponentialStringToNumber(dString,offset,dDefaultVal,endoffset,*)
    Print(offset)
    Print(endoffset)
    Print(dvalue)

    Copy(endoffset, offset)
    ExponentialStringToNumber(dString,offset,dDefaultVal,endoffset,dvalue)
    Print(offset)
    Print(endoffset)
    Print(dvalue)

    Copy(endoffset, offset)
    ExponentialStringToNumber(dString,offset,dDefaultVal,endoffset,dvalue)
    Print(offset)
    Print(endoffset)
    Print(dvalue)

    Copy(endoffset, offset)
    ExponentialStringToNumber(dString,offset,dDefaultVal,endoffset,dvalue)
    Print(offset)
    Print(endoffset)
    Print(dvalue)

    //---------------------------------------------------------

    DecimalStringToNumber(stringa373 * default20 local2 local1)
    Print(local1)

   )
 ) ) )
