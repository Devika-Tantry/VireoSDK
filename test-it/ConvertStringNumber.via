define (default20 dv(.Single 20) )
define (stringa373 dv(.String 'a373') )

define (Test, v(.VirtualInstrument, (
 c(
    e(.Int32 x)
    e(.Int32 y)
    e(v(.Int32,0) c0)
    e(v(.Int32,1) c1)
    e(v(.Int32,2) c2)
    e(v(.Int32,3) c3)
    e(v(.Int32,4) c4)
    e(v(.Int32,5) c5)
    e(v(.Int32,6) c6)
    e(v(.Int32,7) c7)
    e(v(.Int32,8) c8)
    e(v(.Int32,16) c16)
    e(v(.Int32,1234) c1234)
    //--
    e(v(.Int32,-2) cn2)
    e(v(.Int32,-3) cn3)
    //--
    e(.Int32 value)
    e(.Int32 value2)
    e(v(.Int16,4) width)
    e(v(.Int32,12) defaultVal)
    e(v(.Double,1.2) dDefaultVal)
    e(v(.Int32,0) offset)
    e(v(.Int32,0) endoffset)
