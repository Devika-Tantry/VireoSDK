define (Test, v(.VirtualInstrument, (
 c(
    e(v(.Boolean,t) ct)
    e(v(.Boolean,f) cf)
    e(.Boolean x)
    e(.Boolean y)
    e(.Boolean value)
 ),
  2,
  clump(1,
    Copy(cf, x)
    Copy(cf, y)
    Trigger(1)
    Wait(1)
    
    Copy(ct, x)
    Copy(cf, y)
    Trigger(1)
    Wait(1)
   
    Copy(cf, x)
    Copy(ct, y)
    Trigger(1)
    Wait(1)

    Copy(ct, x)
    Copy(ct, y)
    Trigger(1)
    Wait(1)
    //----------------
   )

    clump(1,
    DPrintf("-------------")
    AndBoolean(x,y,value)
    DPrintf(" And (%s %s) => %s" x y value)

    OrBoolean(x,y,value)
    DPrintf(" Or (%s %s) => %s" x y value)

    NorBoolean(x,y,value)
    DPrintf(" Nor (%s %s) => %s" x y value)

    NandBoolean(x,y,value)
    DPrintf(" Nand (%s %s) => %s" x y value)

    XorBoolean(x,y,value)
    DPrintf(" Xor (%s %s) => %s" x y value)

    NotBoolean(x,value)
    DPrintf(" Not (%s) => %s" x value)

    NotBoolean(y,value)
    DPrintf(" Not (%s) => %s" y value)
   )
 ) ) )
enqueue (Test)



