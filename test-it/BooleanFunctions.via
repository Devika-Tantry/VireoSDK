define (Test dv(.VirtualInstrument (
 c(
    e(dv(.Boolean t) ct)
    e(dv(.Boolean f) cf)
    e(.Boolean x)
    e(.Boolean y)
    e(.Boolean value)
 ) 
  clump(1 
    Copy(cf x)
    Copy(cf y)
    Trigger(1)
    Wait(1)
    
    Copy(ct x)
    Copy(cf y)
    Trigger(1)
    Wait(1)
   
    Copy(cf x)
    Copy(ct y)
    Trigger(1)
    Wait(1)

    Copy(ct x)
    Copy(ct y)
    Trigger(1)
    Wait(1)
    //----------------
   )

    clump(1 
    DPrintf("-------------")
    And(x y value)
    DPrintf(" And (%s %s) => %s" x y value)

    Or(x y value)
    DPrintf(" Or (%s %s) => %s" x y value)

    Nor(x y value)
    DPrintf(" Nor (%s %s) => %s" x y value)

    Nand(x y value)
    DPrintf(" Nand (%s %s) => %s" x y value)

    Xor(x y value)
    DPrintf(" Xor (%s %s) => %s" x y value)

    Not(x value)
    DPrintf(" Not (%s) => %s" x value)

    Not(y value)
    DPrintf(" Not (%s) => %s" y value)
   )
 ) ) )
enqueue (Test)



