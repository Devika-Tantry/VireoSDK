define (GlobalVar1 var(.Int32 505))
define (GlobalConst1 dv(.Int32 42))

define (GlobalVar2 var(.String "FiveOhFive"))
define (GlobalConst2 dv(.String "FortyTwo"))


define (HelloTypeValues dv(.VirtualInstrument (
    c(
        e(.GlobalVar1 local1)
        e(.GlobalVar2 local2)
        e(.Type vType)
    )
    clump (1
    
        Printf("-------------")
        // Since the VI uses a Global as the initial value.
        // The VIs data space will pick up the value the global has when
        // the VI is constructed
        Printf("local1 value is  %s, expecting 505 " local1)
        Printf("GlobalVar1 value is  %s, expecting 505 " GlobalVar1)
        Printf("GlobalConst1 value is  %s, expecting 42 " GlobalConst1)
        
//      Copy(local1 GlobalConst1)  cant write on constants
        Copy(GlobalConst1 local1)
        Printf("local1 value is  %s, expecting 42 " local1)
        Printf("GlobalVar1 value is  %s, expecting 505 " GlobalVar1)
        
        Copy(local1 GlobalVar1)
        Printf("local1 value is  %s, expecting 42 " local1)
        Printf("GlobalVar1 value is  %s, expecting 42 " GlobalVar1)  
        
        Printf("-------------")
        TypeOf(GlobalVar1 vType)
        Printf("PrintType for GlobalVar1 <%s>" vType)
        TypeBaseType(vType vType)
        Printf("PrintType for GlobalVar1 base <%s>" vType)

        TypeOf(GlobalConst1 vType)
        Printf("PrintType for GlobalConst1 <%s>" vType)
        TypeBaseType(vType vType)
        Printf("PrintType for GlobalConst1 base <%s>" vType)

        Printf("-------------")
        Printf("local2 value is  %s " local2)
        Printf("GlobalVar2 value is  %s " GlobalVar2)
        Printf("GlobalConst2 value is  %s " GlobalConst2)

        Copy(GlobalConst2 local2)
        Printf("local2 value is %s" local2)
        Printf("GlobalVar2 value is %s" GlobalVar2)

        Copy(local2 GlobalVar2)
        Printf("local2 value is  %s" local2)
        Printf("GlobalVar2 value is  %s" GlobalVar2)  
        Printf("-------------")

        TypeOf(GlobalVar2 vType)
        Printf("PrintType for GlobalVar2 <%s>" vType)
        TypeBaseType(vType vType)
        Printf("PrintType for GlobalVar2 base <%s>" vType)

        TypeOf(GlobalConst2 vType)
        Printf("PrintType for GlobalConst2 <%s>" vType)
        TypeBaseType(vType vType)
        Printf("PrintType for GlobalConst2 base <%s>" vType)


    )
) ) )

enqueue (HelloTypeValues)
