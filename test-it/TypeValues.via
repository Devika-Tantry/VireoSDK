define (HelloWorld v(.VirtualInstrument (
    c( e(v(.String "Hello world. I can fly.") variable1) )
    1
    clump (1

        DPrintf("Boolean  %s " Boolean)

        DPrintf("Int8  %s " Int8)
        DPrintf("Int16  %s " Int16)
        DPrintf("Int32  %s " Int32)
        DPrintf("Int64  %s " Int64)

        DPrintf("UInt8  %s " UInt8)
        DPrintf("UInt16  %s " UInt16)
        DPrintf("UInt32  %s " UInt32)
        DPrintf("UInt64  %s " UInt64)

        DPrintf("Double  %s " Double)
        DPrintf("Single  %s " Single)

        DPrintf("ComplexDouble  %s " ComplexDouble)
        DPrintf("ComplexSingle  %s " ComplexSingle)

        DPrintf("Pi %s" Pi)

        DPrintf("Code Pointer %s" CodePointer)
        DPrintf("Data Pointer %s" DataPointer)
        DPrintf("Clump %s" Clump)

        DPrintf("Virtual Instrument %s" VirtualInstrument)
    )
) ) )
enqueue (HelloWorld)
