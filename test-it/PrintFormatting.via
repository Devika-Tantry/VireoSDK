define (HelloWorld dv(.VirtualInstrument (
    c(
        e(dv(.String "abc") vStr)
        e(dv(.Int32 427) vInt)
        e(dv(.Double 42.4) vReal)
    )
    clump (1
        DPrintf("String with default justify   -----------")
        DPrintf("String no field width          -%s-" vStr)
        DPrintf("String to samll field width    -%1s-" vStr)
        DPrintf("String matching field width    -%3s-" vStr)
        DPrintf("String larger   field width    -%4s-" vStr)
        DPrintf("String xlarge   field width    -%50s-" vStr)

        DPrintf("String with left justify set  -----------")
        DPrintf("String no field width          -%-s-" vStr)
        DPrintf("String to samll field width    -%-1s-" vStr)
        DPrintf("String matching field width    -%-3s-" vStr)
        DPrintf("String larger   field width    -%-4s-" vStr)
        DPrintf("String xlarge   field width    -%-50s-" vStr)

        DPrintf("Int with default justify      -----------")
        DPrintf("Int no field width          -%d-" vInt)
        DPrintf("Int to samll field width    -%1d-" vInt)
        DPrintf("Int matching field width    -%3d-" vInt)
        DPrintf("Int larger   field width    -%4d-" vInt)
        DPrintf("Int xlarge   field width    -%50d-" vInt)

        DPrintf("Int with left justify set     -----------")
        DPrintf("Int no field width          -%-d-" vInt)
        DPrintf("Int to samll field width    -%-1d-" vInt)
        DPrintf("Int matching field width    -%-3d-" vInt)
        DPrintf("Int larger   field width    -%-4d-" vInt)
        DPrintf("Int xlarge   field width    -%-50d-" vInt)

        DPrintf("Real with default justify      -----------")
        DPrintf("Real no field width          -%.0f-" vReal)
        DPrintf("Real to samll field width    -%1.0f-" vReal)
        DPrintf("Real matching field width    -%3.0f-" vReal)
        DPrintf("Real larger   field width    -%4.0f-" vReal)
        DPrintf("Real xlarge   field width    -%50.0f-" vReal)

        DPrintf("Real with left justify set     -----------")
        DPrintf("Real no field width          -%-.0f-" vReal)
        DPrintf("Real to samll field width    -%-1.0f-" vReal)
        DPrintf("Real matching field width    -%-3.0f-" vReal)
        DPrintf("Real larger   field width    -%-4.0f-" vReal)
        DPrintf("Real xlarge   field width    -%-50.0f-" vReal)

        DPrintf("Real with default justify      -----------")
        DPrintf("Real no field width          -%.1f-" vReal)
        DPrintf("Real to samll field width    -%1.1f-" vReal)
        DPrintf("Real matching field width    -%4.1f-" vReal)
        DPrintf("Real larger   field width    -%5.1f-" vReal)
        DPrintf("Real xlarge   field width    -%50.1f-" vReal)

        DPrintf("Real with left justify set     -----------")
        DPrintf("Real no field width          -%-.1f-" vReal)
        DPrintf("Real to samll field width    -%-1.1f-" vReal)
        DPrintf("Real matching field width    -%-4.1f-" vReal)
        DPrintf("Real larger   field width    -%-5.1f-" vReal)
        DPrintf("Real xlarge   field width    -%-50.1f-" vReal)

    )
) ) )
enqueue (HelloWorld)
