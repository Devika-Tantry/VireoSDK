define (SubVIFromMyPast, v(.VirtualInstrument, (
 c(
    e(v(.String 'Yesterday I made scrambled eggs and they seemed far away') str)
  )
  1
  clump(1
    Print(str)
   )
 )))

define (CarpeDiem, v(.VirtualInstrument, (
    c(
        e(v(.String 'Here comes the sun') str)
    )
    1
    clump(1
        SubVIFromMyPast()
        Print(str)
        SubVIFromMyFuture()
    )
)))

define (SubVIFromMyFuture, v(.VirtualInstrument, (
 c(
    e(v(.String 'The sun\'ll come out tomorrow') str)
  )
  1
  clump(1
    Print(str)
   )
 )))

enqueue (CarpeDiem)
